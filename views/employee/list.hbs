<table>
    <thead>
        <tr>
            <th>FUll name</th>
            <th>EMail</th>
            <th>Mobile</th>
            <th>City</th>
        </tr>
    </thead>
    <tbody>
        {{#each list}}
        <tr>
            <td>{{this.fullName}}</td>
            <td>{{this.email</td>
            <td>{{this.mobile}}</td>
            <td>{{this.city}}</td>
            <td>
                <a href="/employee/{{this._id}}"><i>Edit</i></a>
                <a href="/employee/delete/{{this._id}}"><i>Delete</i></a>
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>